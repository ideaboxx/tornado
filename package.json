{
  "name": "tornedo",
  "version": "1.0.0",
  "proxy": "http://localhost:8080",
  "scripts": {
    "postcss:watch": "postcss ./src/style.css -o ./src/App.css -w",
    "postcss": "postcss ./src/style.css -o ./src/App.css",
    "react:start": "react-scripts start",
    "start": "run-p postcss:watch react:start",
    "build": "npm run postcss && react-scripts build && rm -rf ./server/build && mv ./build ./server"
  },
  "repository": {
    "type": "git",
    "url": "https://git.heroku.com/tornedo.git"
  },
  "author": "tilak",
  "dependencies": {
    "@ideabox/cloud-drive-fs": "^0.2.7",
    "autoprefixer": "^9.8.0",
    "express": "^4.16.4",
    "googleapis": "^100.0.0",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.1",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^2.1.3",
    "rimraf": "^2.6.3",
    "tailwindcss": "^3.0.24",
    "webtorrent": "^0.116.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "npm-run-all": "^4.1.5"
  }
}
